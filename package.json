{
	"name": "open",
	"type": "module",
	"private": true,
	"version": "0.1.1",
	"description": "React Native Assignment",
	"author": "Pavel Ravits",
	"homepage": "",
	"license": "MIT",
	"keywords": [
		"react",
		"react-native"
	],
	"engines": {
		"node": ">=18.x",
		"pnpm": ">=8.x"
	},
	"scripts": {
		"build": "pnpm -r build",
		"lint": "eslint packages/*/src --ext .ts,.tsx --cache",
		"format": "prettier --write packages/**/src --cache",
		"format:check": "prettier --check packages/**/**/src --cache",
		"format:write": "prettier --write packages/**/**/src --cache",
		"typecheck": "tsc --noEmit",
		"test": "vitest run"
	},
	"preinstall": "npx only-allow pnpm",
	"dependencies": {
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.1.2",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.7.3",
		"@vitest/coverage-v8": "^0.34.6",
		"eslint": "^8.50.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prefer-arrow-functions": "^3.1.4",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-unused-imports": "^3.0.0",
		"jsdom": "^23.0.1",
		"jsonc-eslint-parser": "^2.4.0",
		"prettier": "^3.0.3",
		"ts-pattern": "^5.0.5",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"devDependencies": {
		"@types/swagger-ui-express": "^4.1.8"
	}
}
